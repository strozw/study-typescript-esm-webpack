{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"name": "study-typescript-esm-webpack",
	"main": "dist/index.js",
	"type": "module",
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		]
	},
	"scripts": {
		"webpack": "node --experimental-specifier-resolution=node --loader ts-node/esm ./node_modules/webpack-cli/bin/cli.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "npm run webpack serve -- --mode development",
		"build": "npm run webpack build -- --mode production"
	},
	"devDependencies": {
