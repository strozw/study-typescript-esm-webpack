		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"babel-loader": "^8.2.5",
		"core-js": "^3.23.4",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^28.1.3",
		"prettier": "^2.7.1",
		"react-refresh": "^0.14.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.10.0",
